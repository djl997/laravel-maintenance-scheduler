{
    "name": "djl997/laravel-maintenance-scheduler",
    "description": "Package to manage application version, schedule maintenance, version changelog and maintenance mode.",
	"keywords": ["laravel", "schedule-maintenance", "maintenance-mode"],    
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Djl997\\LaravelMaintenanceScheduler\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dirk Jan",
            "email": "dirkjan@q2-software.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Djl997\\LaravelMaintenanceScheduler\\LaravelMaintenanceSchedulerServiceProvider"
            ]
        }
    }
}
